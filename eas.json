{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "internal": {
      "distribution": "internal",
      "developmentClient": false,
      "channel": "internal"
    },
    "production": {
      "distribution": "store",
      "developmentClient": false,
      "autoIncrement": true,
      "channel": "production"
    },
    "production-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
